{
    "title": "个人笔记",
    "description": "日常日志记录",
    "author": "ycxv",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "常用网址": "http://www.633521.com"
        }
    },
    "plugins": [
        "-highlight",
        "search-plus@^0.0.11",
        "simple-page-toc@^0.1.1",
        "github@^2.0.0",
        "github-buttons@2.1.0",
        "edit-link@^2.0.2",
        "disqus@^0.1.0",
        "prism@^2.1.0",
        "prism-themes@^0.0.2",
        "advanced-emoji@^0.2.1"
    ],

    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ]
        },
        "github": {
            "url": "https://github.com/mickYue/gitbook_blog"
        },
        "github-buttons": {
            "repo": "/mickYue/gitbook_blog",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "edit-link": {
            "base": "https://github.com/mickYue/gitbook_blog/edit/master",
            "label": "Edit This Page"
        }
    }
}
